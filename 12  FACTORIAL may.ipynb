   ],
   "source": [
    "n=int(input(\"ENTER NUMBER: \"))\n",
    "f=1                   # intialization\n",
    "while n!=1:\n",
    "    f=f*n\n",
    "    print(n,\"x \",end='')  # for formula implementation\n",
    "    n=n-1\n",
    "print(n,\"=\",f)        # to print last number"
   ]
